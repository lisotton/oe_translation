<?xml version="1.0" encoding="UTF-8"?>
<POETRY xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="">
  <request communication="synchrone" id="1085784" type="status">
    <demandeId>
      <codeDemandeur>{{ request_identifier.code }}</codeDemandeur>
      <annee>{{ request_identifier.year }}</annee>
      <numero>{{ request_identifier.number }}</numero>
      <version>{{ request_identifier.version }}</version>
      <partie>{{ request_identifier.part }}</partie>
      <produit>{{ request_identifier.product }}</produit>
    </demandeId>
    <status code="0" type="request">
      <statusDate>02/08/2019</statusDate>
      <statusTime>10:32:01</statusTime>
      <statusMessage>OK</statusMessage>
    </status>
    <status code="{{ request_status }}" type="demande">
      <statusDate>02/08/2019</statusDate>
      <statusTime>10:31:44</statusTime>
      <statusMessage></statusMessage>
    </status>
    {% for accepted_language in accepted_languages %}
      <status code="ONG" lgCode="{{ accepted_language.code }}" type="attribution">
        <statusDate>08/02/2019</statusDate>
        <statusTime>00:00:00</statusTime>
      </status>
    {% endfor %}
    {% for refused_language in refused_languages %}
      <status code="REF" lgCode="{{ refused_language.code }}" type="attribution">
        <statusDate>08/02/2019</statusDate>
        <statusTime>00:00:00</statusTime>
      </status>
    {% endfor %}
    {% for cancelled_language in cancelled_languages %}
      <status code="CNL" lgCode="{{ cancelled_language.code }}" type="attribution">
        <statusDate>08/02/2019</statusDate>
        <statusTime>00:00:00</statusTime>
      </status>
    {% endfor %}
    {% for accepted_language in accepted_languages %}
      <attributions format="HTML" lgCode="{{ accepted_language.code }}">
        <attributionsDelai>{{ accepted_language.date }}</attributionsDelai>
        <attributionsDelaiAccepted>{{ accepted_language.accepted_date }}</attributionsDelaiAccepted>
      </attributions>
    {% endfor %}
  </request>
</POETRY>
